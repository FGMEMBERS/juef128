<?xml version="1.0"?>

<!-- Station Select Panel -->

<PropertyList>

 <path>weapons-panel.ac</path>


 <!-- Station 1 switch -->
 <animation>
  <type>select</type>
  <object-name>sw1-on</object-name>
   <condition>
    <and>
    <equals>
     <property>fdm/jsbsim/systems/armament/station1/selected</property>
     <value>1</value>
    </equals>
    <not>
			<property>fdm/jsbsim/systems/armament/station1/released</property>
    </not>
    </and>
   </condition> 
 </animation>  
 <animation>
  <type>material</type>
  <object-name>sw1-on</object-name>
   <condition>
    <and>
    <equals>
     <property>fdm/jsbsim/systems/armament/station1/selected</property>
     <value>1</value>
    </equals>
    <not>
			<property>fdm/jsbsim/systems/armament/station1/released</property>
    </not>
    </and>
   </condition> 
　　　<emission>
　　　　<red>1</red>
　　　　<green>1</green>
　　　　<blue>1</blue>
　　　</emission>
 </animation>  

